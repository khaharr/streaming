 /src/main/java
   fr.gouv.finances.monappli.monappliapi
               |-- api.v1
               |   |-- controller                         - Les contrôleurs REST
               |   |-- dto                                - Les DTOs
               |-- config                                 - Les classes de configuration (Jersey, Spring Security, Tomcat ...)
               |-- dao                                    - Les DAOs
               |-- entite                                 - Les entités
               |-- service                                - Les services métiers
               |-- techbean                               - Les beans techniques    
   org.springframework.boot.autoconfigure.web
   client-config.wsdd
 /src/main/resources
               |-- conf                                  - La configuration XML des beans
               |-- data                                  - Les données
               |-- ldap                                  - La configuration du LDAP embarqué (fichier LDIF)
               |-- META-INF
               |   |-- persistence.xml                   - Le fichier de persistance JPA
               |-- application.properties                - Le fichier de configuration Spring Boot
               |-- banner.txt                            - Le bandeau
               |-- log4j2.xml                            - Le fichier de configuration des logs
 /src/test/java
 /src/main/webapp
               |-- aide
               |-- application
               |-- META-INF
               |-- WEB-INF
               |   |-- openapi.yaml                       - Le fichier de définition des endpoints (API REST)
               |   |-- web.xml                            - Le descripteur de déploiement
               |-- lignedevie.jsp                         - JSP de ligne de vie
 /pom.xml
